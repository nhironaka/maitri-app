</tr>
<tr>
<td class="title">Start Date</td>
<td class="data"><%= @patient.start_date %></td>
<td class="title">End Date</td>
<td class="data"><%= @patient.end_date %></td>
</tr>
<tr>
<td class="title">DOB</td>
<td class="data">MM/DD/YYYY</td>
<td class="title">RELATIVES</td>
<td class="data">AUNTS, UNCLES</td>
</tr>
<tr>
<td class="title">AGE</td>
<td class="data">33</td>
<td class="title">MARITAL STATUS</td>
<td class="data">MARRIED? SINGLE?</td>
</tr>
<tr>
<td class="title">RESIDENCE</td>
<td class="data">PLACE</td>
<td class="title">SPOUSE</td>
<td class="data">SIGNIFICANT OTHER</td>
</tr>
</table>

<br/><br/>

<span class="section_header style2">APPEARANCE</span>

<br/>

<table cellspacing="5px" class="patient_record_table">
<tr>
<td class="title">HEIGHT</td>
<td class="data">#'##"</td>
<td class="title">HAIR</td>
<td class="data">COLOR, LENGTH</td>
</tr>
<tr>
<td class="title">BUILD</td>
<td class="data">BUILD TYPE</td>
<td class="title">EYES</td>
<td class="data">COLOR</td>
</tr>
</table>

<br/><br/>

PHYSICAL DESCRIPTION
