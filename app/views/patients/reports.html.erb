<div class="content">
  <h1>Reports</h1>
  <div class="panels" id="index_panels">
    <div class="panel_container index" id="panel_filter">
      <%= button_to download_report_patients_path, :id=>"download_button", :method=>:post do %>
      <div class="panel shade_1">
        <img class="icon icons8-Combo-Chart" src="data:image/png;base64," width="50" height="50">
        <div class="panel_content">Create a Report</div></div><%end%>
    </div>
    <div class="panel_container index" id="panel_reset"><button onclick="reset()" type="button" id="generate_button" ><div class="panel shade_2">
      <img class="icon icons8-Download-From-Ftp" src="data:image/png;base64," width="50" height="50">
      <div class="panel_content">Generate</div></div></button>
    </div>
    <div class="panel_container index"><button type="button" id="import_button"><div class="panel shade_3">
      <img class="icon icons8-Upload" src="data:image/png;base64," width="50" height="50">
      <div class="panel_content">Filler</div></div></button>
    </div>
  </div>
  <div class="filters">
    <%= render :partial=>"/patients/reports.html.erb" %>  
  </div>
</div>


