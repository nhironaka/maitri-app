<div class="table_container">
  <div class="table_header_container" id="table_header_">
    <table id="table_header">
      <thead>
        <tr>
          <th style="border-color:white"><%= sortable "Name" %></th>
          <th><%= sortable "Gender" %></th>
          <th><%= sortable "start_date" %></th>
          <th><%= sortable "end_date" %></th>
          <th>Housing</th>
          <th>Income</th>
          <th>SSN</th>
          <th>DOB</th>
        </tr>
      </thead>
    </table>
  </div>
  <div class="scrolling_table" id="patients_table_" onscroll="synch_scroll()">
    <table id="patients_table">
      <tbody>
        <% if not @patients.nil? %>
          <% @patients.each do |patient| %>
            <tr>
                <td style="border-color:white"><%= patient.name %></td>
                <td><%= patient.gender %></td>
                <td><%= patient.start_date.strftime("%Y-%b-%d") %></td>
                <td><%= patient.end_date.strftime("%Y-%b-%d") %></td> 
                <td></td><td></td><td></td><td></td>
            </tr>
          <% end %>
        <% end %>
     </tbody>
    </table>
  </div> 
</div>
